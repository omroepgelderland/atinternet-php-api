{
    "name": "omroepgelderland/atinternet-php-api",
    "description": "PHP implementation for the Piano Reporting API.",
    "type": "library",
        "keywords": [
            "at internet",
            "piano analytics"
        ],
    "license": "MIT",
    "authors": [
        {
            "name": "Remy Glaser",
            "email": "rglaser@gld.nl"
        }
    ],
    "minimum-stability": "dev",
	"prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "atinternet_php_api\\": "src/"
        },
        "files": [
            "src/exceptions.php"
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12"
    }
}
